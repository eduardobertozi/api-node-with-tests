{
  "name": "api-node",
  "version": "0.0.0",
  "description": "A simple node api",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "test": "npm run test:unit && npm run test:integration",
    "test:integration": "mocha --config=test/integration/.mocharc.json --node-env=test  test/integration/**/*.test.js",
    "test:unit": "mocha --config=test/unit/.mocharc.json --node-env=test  test/unit/**/*.test.js"
  },
  "keywords": [
    "nodejs",
    "node",
    "backend"
  ],
  "author": "Eduardo Bertozi",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.2",
    "config": "^3.3.9",
    "express": "^4.18.2",
    "express-acl": "^2.0.8",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^6.10.0"
  },
  "devDependencies": {
    "chai": "^4.3.7",
    "mocha": "^10.2.0",
    "sinon": "^9.2.4",
    "supertest": "^6.3.3"
  }
}
